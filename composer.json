{
    "name": "idez/laravel-cnab",
    "description": "Laravel Cnab",
    "keywords": [
        "idezdigital",
        "laravel",
        "cnab"
    ],
    "homepage": "https://github.com/idezdigital/cnab",
    "license": "MIT",
    "authors": [
        {
            "name": "Samuel Cecilio",
            "email": "samuel@idez.com.br",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0",
        "illuminate/contracts": "^8.37",
        "spatie/laravel-package-tools": "^1.4.3"
    },
    "require-dev": {
        "brianium/paratest": "^6.3",
        "friendsofphp/php-cs-fixer": "^2.16",
        "nunomaduro/collision": "^5.3",
        "nunomaduro/larastan": "^1.0",
        "pestphp/pest": "^1.18",
        "pestphp/pest-plugin-laravel": "^1.1",
        "pestphp/pest-plugin-parallel": "^0.3.1",
        "phpstan/extension-installer": "^1.1",
        "phpstan/phpstan-deprecation-rules": "^1.0",
        "phpstan/phpstan-phpunit": "^1.0",
        "phpunit/phpunit": "^9.3"
    },
    "autoload": {
        "psr-4": {
            "Idez\\Cnab\\": "src/",
            "Database\\Factories\\": "database/factories/"
        },
        "files": [
            "src/Helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests"
        }
    },
    "scripts": {
        "format": "vendor/bin/php-cs-fixer fix --allow-risky=yes",
        "phpstan": "vendor/bin/phpstan analyse",
        "test": "vendor/bin/testbench package:test --parallel --no-coverage",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Idez\\Cnab\\CnabServiceProvider"
            ],
            "aliases": {
                "Cnab": "Idez\\Cnab\\CnabFacade"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
